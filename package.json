{
  "name": "medianext-ts",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "private": "true",
  "scripts": {
    "build": "swc src -d build",
    "watch": "swc src -d build -w",
    "dev": "run-s build start",
    "watch:start": "nodemon",
    "start": "node --preserve-symlinks --enable-source-maps .",
    "clean": "node scripts/clean.mjs",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext ts --fix"
  },
  "imports": {
    "#root/*": "./build/*.js",
    "#lib/*": "./build/lib/*.js",
    "#constants/*": "./build/lib/constants/*.js",
    "#env/*": "./build/lib/env/*.js",
    "#utils/*": "./build/lib/utils/*.js"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@sapphire/eslint-config": "^4.3.5",
    "@sapphire/prettier-config": "^1.4.3",
    "@sapphire/ts-config": "^3.3.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "1.2.189",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@sapphire/decorators": "^4.3.4",
    "@sapphire/fetch": "^2.3.1",
    "@sapphire/framework": "next",
    "@skyra/env-utilities": "^0.0.7",
    "colorette": "^2.0.16",
    "discord-api-types": "^0.33.0",
    "discord.js": "^13.7.0",
    "fastify": "^3.29.0",
    "ioredis": "^5.0.5",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.0"
  },
  "engines": {
    "node": ">=16.x"
  },
  "prettier": "@sapphire/prettier-config",
  "eslintConfig": {
      "extends": "@sapphire",
      "ignorePatterns": ["node_modules/", "*build/", "*dist/", "*prod/"]
  }
}
