{
  "name": "medianext-ts",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "private": "true",
  "scripts": {
    "build": "swc src -d build",
    "watch": "swc src -d build -w",
    "dev": "run-s build start",
    "watch:start": "tsc-watch --on-Success \"npm start\"",
    "start": "node --preserve-symlinks --enable-source-maps .",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext ts --fix"
  },
  "type": "module",
  "imports": {
    "#root/*": "build/*.js",
    "#lib/*": "build/lib/*.js",
    "#utils/*": "build/lib/modules/*.js",
    "#env/*": "build/lib/env/*.js"
  },
  "author": "",
  "license": "ISC",
  "prettier": "@sapphire/prettier-config",
  "devDependencies": {
    "@sapphire/eslint-config": "^4.3.3",
    "@sapphire/prettier-config": "^1.4.2",
    "@sapphire/ts-config": "^3.3.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.167",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@sapphire/decorators": "^4.3.4",
    "@sapphire/fetch": "^2.3.1",
    "@sapphire/framework": "next",
    "@skyra/env-utilities": "^0.0.7",
    "colorette": "^2.0.16",
    "discord-api-types": "^0.31.1",
    "discord.js": "^13.6.0",
    "fastify": "^3.28.0",
    "ioredis": "^5.0.4",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.1"
  },
  "engines": {
    "node": ">=16.x"
  }
}
